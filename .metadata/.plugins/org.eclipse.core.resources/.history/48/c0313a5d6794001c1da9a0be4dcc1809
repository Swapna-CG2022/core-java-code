package mathematicaloperations;

public class IncrementOperators {

	public static void main(String[] args) {
		
		int a = 5;
		int b = 6;
		int value = a++ + b++;
		System.out.println( "a value = " + a++);
		a = 5;
		System.out.println( "a value = " + -- a);
		System.out.println( "a value above print stmt = " + a);
	}

}
